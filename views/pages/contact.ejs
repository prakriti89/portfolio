<section id="contact" class="container content-section text-center">
    <div class="row">
        <h1>Contact Details</h1>
        <p>Feel free to email me to get more information, or to just say hello!</p>
        <p><a href="mailto:prakriti89@gmail.com">prakriti89@gmail.com</a>
        </p>
        <ul class="list-inline banner-social-buttons">
            <% socialMedias.forEach(function(media) { %> 
                <li>
                    <a href="<%= media.url %>" class="btn btn-default btn-lg">
                        <i class="fa <%= media.icon %> fa-fw"></i> 
                        <span class="network-name"><%= media.name %></span>
                    </a>
                </li>
            <% }); %>
        </ul>
    </div>
</section>